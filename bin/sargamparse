#!/usr/bin/env node
var path = require('path');
var fs = require('fs');
var lib = path.join(path.dirname(fs.realpathSync(__filename)), '../lib');
require(lib+'/sargam/sargam_parser.js')
var parser=SargamParser
var source= fs.readFileSync('/dev/stdin').toString()
var composition=parser.parse(source)
composition.source=source
process.stdout.write(sys.inspect(composition,false,null)+"\n")
